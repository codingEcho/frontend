<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.5.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>jqTriggerHandler</Title>
      <Author>John Sheehan (@johnsheehan)</Author>
      <Description>Execute all handlers attached to an element for an event.</Description>
      <HelpUrl>https://github.com/kspearrin/Visual-Studio-jQuery-Code-Snippets</HelpUrl>
      <Shortcut>jqTriggerHandler</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal Editable="true">
          <ID>eventType</ID>
          <ToolTip>A string containing a JavaScript event type, such as click or submit.</ToolTip>
          <Default>eventType</Default>
          <Function>
          </Function>
        </Literal>
        <Literal Editable="true">
          <ID>selector</ID>
          <ToolTip>Element(s) to bind to</ToolTip>
          <Default>selector</Default>
          <Function>
          </Function>
        </Literal>
      </Declarations>
      <Code Language="javascript"><![CDATA[$$($selector$).triggerHandler($eventType$);$end$]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>