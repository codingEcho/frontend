<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.4.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>am-btn-group-sm</Title>
      <Author>Minqing Luo</Author>
      <Description>Amaze UI CSS Button Group am-btn-group-sm</Description>
      <Shortcut>am-btn-group-sm</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal Editable="true">
          <ID>content</ID>
          <ToolTip>按钮显示的内容</ToolTip>
          <Default>按钮组</Default>
          
          
        </Literal>
      </Declarations>
      <Code Language="html">
        <![CDATA[
<div class="am-btn-group am-btn-group-sm">
    <button type="button" class="am-btn am-btn-default">$content$</button>  
</div>
        $end$
        ]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>