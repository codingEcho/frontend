<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.4.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>am-animation-fade</Title>
      <Author>Mingqing Luo</Author>
      <Description>Amaze UI Animation Fade</Description>
      <Shortcut>am-animation-fade</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal Editable="true">
          <ID>reverse</ID>
          <ToolTip>反向执行CSS类</ToolTip>
          <Default>am-animation-reverse</Default>
          
          
        </Literal>
      </Declarations>
      <Code Language="html">
        <![CDATA[
        <div class="am-animation-fade $reverse$">fade</div>
        $end$
        ]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>