<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.4.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>jQuery Script CDN BAIDU</Title>
      <Author>Ming Luo</Author>
      <Description>A script tag with a jQuery src using BAIDU's CDN.</Description>
      <HelpUrl>https://github.com/kspearrin/Visual-Studio-jQuery-Code-Snippets</HelpUrl>
      <Shortcut>jqScriptCdnBaidu</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal Editable="true">
          <ID>version</ID>
          <ToolTip>Library version number</ToolTip>
          <Default>2.1.1</Default>
          <Function>
          </Function>
        </Literal>
      </Declarations>
      <Code Language="html">
        <![CDATA[<script type="text/javascript" src="http://apps.bdimg.com/libs/jquery/$version$/jquery.min.js"></script>
$end]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>